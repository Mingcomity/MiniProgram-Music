/* components/area-header/area-header.wxss */
.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 24rpx 0;
}
.header .title {
  font-size: 36rpx;
  font-weight: 600;
  color: #000;
}
.header .more {
  font-size: 30rpx;
  display: flex;
  align-items: center;
  color: #666666;
  gap: 10rpx;
}
.header .more .text {
  font-size: 28rpx;
}